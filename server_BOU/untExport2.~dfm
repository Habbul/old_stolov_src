  object qCreditForPers: TMySQLQuery
    DatabaseName = 'beznal'
    Server = frmSpr.MySQLServer
    SQL.Strings = (
      
        'SELECT concat(p.family, " ", p.name, " ", p.parentname), p.tabnu' +
        'm, p.flagkredita,'
      'c.nomer, c.statecard_id, cr.datebegin, cr.dateend, cr.sumcredit'
      'FROM pers p'
      'left join card c on c.card_id=p.card_id'
      'left join credits cr on cr.card_id=p.card_id'
      'where instr(p.family, '#39#1082#1072#1088#1080#1084#1086#39')')
    Left = 48
    Top = 88
  end
